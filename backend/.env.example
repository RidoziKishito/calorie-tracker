# Copy to .env and fill in your Supabase Postgres connection string
# Supabase DATABASE_URL format example:
# postgresql://postgres:<PASSWORD>@db.<PROJECT_REF>.supabase.co:5432/postgres
# or (pooler) postgresql://postgres:<PASSWORD>@aws-0-ap-southeast-1.pooler.supabase.com:5432/postgres

DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres

# JWT / Auth settings (keep or adjust)
SECRET_KEY=replace-with-a-very-long-random-string
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# FastAPI port (optional override)
API_PORT=8000
